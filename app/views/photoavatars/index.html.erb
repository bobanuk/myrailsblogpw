<p id="notice"><%= notice %></p>

<h1>Listing Photoavatars</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Content</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @photoavatars.each do |photoavatar| %>
      <tr>
        <td><%= photoavatar.title %></td>
        <td><%= photoavatar.content %></td>
        <td><%= link_to 'Show', photoavatar %></td>
        <td><%= link_to 'Edit', edit_photoavatar_path(photoavatar) %></td>
        <td><%= link_to 'Destroy', photoavatar, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Photoavatar', new_photoavatar_path %>
